nlist=[]
n=int(input("enter total no  of elements"))
for i in range(1,n+1):
    val=int(input("enter the element"))
    nlist.append(val)
for i in range(n):
    for j in range(i+1,n):
        if(nlist[i]<nlist[j]):
            t=nlist[i]
            nlist[i]=nlist[j]
            nlist[j]=t

print("the descending orderÂ is:",nlist)
