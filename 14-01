n=int(input("enter a num of values:"))
num=int(input("enter a number:"))
    
maximum=num
minimum=maximum
for i in range(1,n):
    num=int(input("enter a number"))  
    if  num>maximum:
                  maximum=num
    if num<minimum:
                  minimum=num
print("maximum is",maximum)
print("minimumÂ is",minimum)
